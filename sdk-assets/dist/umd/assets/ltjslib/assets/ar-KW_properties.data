###################################################
#      Date and Time Patterns
###################################################
# Date - Long/Medium/Short
datePatternLong=EEEE، d MMMM yyyy
datePatternMedium=d MMMM yyyy
datePatternShort=dd/MM/yyyy
 
# Time - Long/Medium/Short
timePatternLong=hh:mm:ss a zzz
timePatternMedium=hh:mm:ss a
timePatternShort=hh:mm a
 
# Date Format
# Used for DATE and DTDATE
date.0=d/M/yyyy
date.1=dd/MM/yyyy
date.2=d MMMM yyyy
date.3=EEEE، d MMMM yyyy
 
# Day Format
# Used for DAY
day.0=d
 
# DOWNAME Format
# Used for DOWNAME
downame.0=ccc
downame.1=cccc
 
# MMDDYY Format
# Used for MMDDYY, DDMMYY and YYMMDD
mmddyy.0=d/M/yyyy
mmddyy.1=dd/MM/yyyy
 
# MMYY Format
# Used for MMYY and YYMM
mmyy.0=M/yyyy
mmyy.1=MMM yyyy
 
# MONTH Format
# Used for MONTH
monname.0=M
monname.1=LLL
monname.2=LLLL
 
# MONYY Format
# Used for MONYY, YYMON and DTMONYY
monyy.0=MMM yy
monyy.1=MMM yyyy
monyy.2=MMMM yyyy
 
# WEEKDATE Format
# Used for WEEKDATE, WEEKDATX and DTWKDATX
weekdate.0=ccc
weekdate.1=cccc
weekdate.2=EEE، d MMM yyyy
weekdate.3=EEEE، d MMMM yyyy
 
# WORDDATE Format
# Used for WORDDATE and WORDDATX
worddate.0=LLL
worddate.1=LLLL
worddate.2=dd/MM/yyyy
worddate.3=d MMMM yyyy
 
# YEAR Format
# Used for YEAR
year.0=yy
year.1=yyyy
 
# DATETIME Format
# Used for DATETIME, DATEAMPM
datetime.0=d/M/yyyy, hh:mm a
datetime.1=dd/MM/yyyy, hh:mm:ss a
datetime.2=d MMMM yyyy في hh:mm:ss a zzz
datetime.3=EEEE، d MMMM yyyy في hh:mm:ss a zzz
 
# TIME Format
# Used for TOD, TIME and TIMEAMPM
time.0=HH
time.1=hh:mm a
time.2=hh:mm:ss a
time.3=hh:mm:ss a zzz
 
# HOUR Format
# Used for HOUR
hour.0=HH
hour.1=hh a
 
# HHMM Format
# Used for HHMM
hhmm.0=HH:mm
hhmm.1=hh:mm a
 
# MMSS Format
# Used for MMSS
mmss.0=mm
mmss.1=mm:ss
 
# Quarter Format
# Used by YYQC and DTYYQC
quarteryear.0=QQQ yyyy
quarteryear.1=QQQQ yyyy
 
